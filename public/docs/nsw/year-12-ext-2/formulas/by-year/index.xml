      <guid>http://localhost:1313/AustralianSchoolMaths/docs/nsw/year-12-ext-2/formulas/by-year/proof/</guid>
      <description>&lt;div class=&#34;book-tabs&#34;&gt;&#xA;&#xA;&#xA;&#xA;&lt;input type=&#34;radio&#34; class=&#34;toggle&#34; name=&#34;tabs-af149b09&#34; id=&#34;tabs-af149b09-0&#34; checked=&#34;checked&#34; /&gt;&#xA;&lt;label for=&#34;tabs-af149b09-0&#34;&gt;Standard view&lt;/label&gt;&#xA;&lt;div class=&#34;book-tabs-content markdown-inner&#34;&gt;&#xA;&#xA;&lt;style type=&#34;text/css&#34;&gt;&#xA;#T_standard_e403ab17ebb3dfda th.col_heading {&#xA;  text-align: left;&#xA;  font-size: 1em;&#xA;}&#xA;#T_standard_e403ab17ebb3dfda td {&#xA;  text-align: left;&#xA;  font-size: 1em;&#xA;  padding: 1.5em;&#xA;}&#xA;&lt;/style&gt;&#xA;&lt;table id=&#34;T_standard_e403ab17ebb3dfda&#34;&gt;&#xA;  &lt;thead&gt;&#xA;  &lt;/thead&gt;&#xA;  &lt;tbody&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_e403ab17ebb3dfda_row0_col0&#34; class=&#34;data row0 col0&#34; &gt;$ |a| \ge a,  \forall \: a \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_e403ab17ebb3dfda_row1_col0&#34; class=&#34;data row1 col0&#34; &gt;$ |a||b| = |ab|,  \forall \: a,b \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_e403ab17ebb3dfda_row2_col0&#34; class=&#34;data row2 col0&#34; &gt;$ |a|^2 = a^2,  \forall \: a \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_e403ab17ebb3dfda_row3_col0&#34; class=&#34;data row3 col0&#34; &gt;$ |ab| \ge ab,  \forall \: a,b \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_e403ab17ebb3dfda_row4_col0&#34; class=&#34;data row4 col0&#34; &gt;$ a^2 \ge b^2 \iff |a| \ge |b|,  \forall \: a,b \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_e403ab17ebb3dfda_row5_col0&#34; class=&#34;data row5 col0&#34; &gt;$ |x| + |y| \ge |x+y|, \forall \; x, y \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_e403ab17ebb3dfda_row6_col0&#34; class=&#34;data row6 col0&#34; &gt;$ \text {even number } 2n, \forall \: n \in \mathbb{N}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_e403ab17ebb3dfda_row7_col0&#34; class=&#34;data row7 col0&#34; &gt;$ \text {odd number } 2n-1, \forall \: n \in \mathbb{N}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_e403ab17ebb3dfda_row8_col0&#34; class=&#34;data row8 col0&#34; &gt;$ \text {square number } n^2, \forall \: n \in \mathbb{N}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_e403ab17ebb3dfda_row9_col0&#34; class=&#34;data row9 col0&#34; &gt;$ \text {X is divisible by p when there exists another number, } p, p \in \mathbb{N}, \text{ if } \exists \; Y, Y \in \mathbb{N} \text { such that } X=pY$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;  &lt;/tbody&gt;&#xA;&lt;/table&gt;&#xA;&lt;/div&gt;&#xA;&#xA;&#xA;&#xA;&lt;input type=&#34;radio&#34; class=&#34;toggle&#34; name=&#34;tabs-af149b09&#34; id=&#34;tabs-af149b09-1&#34;  /&gt;&#xA;&lt;label for=&#34;tabs-af149b09-1&#34;&gt;Proofs required&lt;/label&gt;&#xA;&lt;div class=&#34;book-tabs-content markdown-inner&#34;&gt;&#xA;&#xA;Items where proofs required are highlighted &#xA;&lt;br&gt;&#xA;&lt;style type=&#34;text/css&#34;&gt;&#xA;#T_proof_required_5d2a9eb2d059c04a th.col_heading {&#xA;  text-align: left;&#xA;  font-size: 1em;&#xA;}&#xA;#T_proof_required_5d2a9eb2d059c04a td {&#xA;  text-align: left;&#xA;  font-size: 1em;&#xA;  padding: 1.5em;&#xA;}&#xA;#T_proof_required_5d2a9eb2d059c04a_row0_col0, #T_proof_required_5d2a9eb2d059c04a_row1_col0, #T_proof_required_5d2a9eb2d059c04a_row2_col0, #T_proof_required_5d2a9eb2d059c04a_row3_col0, #T_proof_required_5d2a9eb2d059c04a_row4_col0, #T_proof_required_5d2a9eb2d059c04a_row6_col0, #T_proof_required_5d2a9eb2d059c04a_row7_col0, #T_proof_required_5d2a9eb2d059c04a_row8_col0, #T_proof_required_5d2a9eb2d059c04a_row9_col0 {&#xA;  background-color: rgba(0,0,0,0);&#xA;}&#xA;#T_proof_required_5d2a9eb2d059c04a_row5_col0 {&#xA;  background-color: rgba(0,150,200, 0.2);&#xA;}&#xA;&lt;/style&gt;&#xA;&lt;table id=&#34;T_proof_required_5d2a9eb2d059c04a&#34;&gt;&#xA;  &lt;thead&gt;&#xA;  &lt;/thead&gt;&#xA;  &lt;tbody&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_proof_required_5d2a9eb2d059c04a_row0_col0&#34; class=&#34;data row0 col0&#34; &gt;$ |a| \ge a,  \forall \: a \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_proof_required_5d2a9eb2d059c04a_row1_col0&#34; class=&#34;data row1 col0&#34; &gt;$ |a||b| = |ab|,  \forall \: a,b \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_proof_required_5d2a9eb2d059c04a_row2_col0&#34; class=&#34;data row2 col0&#34; &gt;$ |a|^2 = a^2,  \forall \: a \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_proof_required_5d2a9eb2d059c04a_row3_col0&#34; class=&#34;data row3 col0&#34; &gt;$ |ab| \ge ab,  \forall \: a,b \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_proof_required_5d2a9eb2d059c04a_row4_col0&#34; class=&#34;data row4 col0&#34; &gt;$ a^2 \ge b^2 \iff |a| \ge |b|,  \forall \: a,b \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_proof_required_5d2a9eb2d059c04a_row5_col0&#34; class=&#34;data row5 col0&#34; &gt;$ |x| + |y| \ge |x+y|, \forall \; x, y \in \mathbb{R}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_proof_required_5d2a9eb2d059c04a_row6_col0&#34; class=&#34;data row6 col0&#34; &gt;$ \text {even number } 2n, \forall \: n \in \mathbb{N}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_proof_required_5d2a9eb2d059c04a_row7_col0&#34; class=&#34;data row7 col0&#34; &gt;$ \text {odd number } 2n-1, \forall \: n \in \mathbb{N}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_proof_required_5d2a9eb2d059c04a_row8_col0&#34; class=&#34;data row8 col0&#34; &gt;$ \text {square number } n^2, \forall \: n \in \mathbb{N}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_proof_required_5d2a9eb2d059c04a_row9_col0&#34; class=&#34;data row9 col0&#34; &gt;$ \text {X is divisible by p when there exists another number, } p, p \in \mathbb{N}, \text{ if } \exists \; Y, Y \in \mathbb{N} \text { such that } X=pY$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;  &lt;/tbody&gt;&#xA;&lt;/table&gt;&#xA;&lt;/div&gt;&#xA;&#xA;&#xA;&lt;/div&gt;</description>
    </item>
    <item>
      <title></title>
      <link>http://localhost:1313/AustralianSchoolMaths/docs/nsw/year-12-ext-2/formulas/by-year/complex-numbers/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>http://localhost:1313/AustralianSchoolMaths/docs/nsw/year-12-ext-2/formulas/by-year/complex-numbers/</guid>
      <description>&lt;div class=&#34;book-tabs&#34;&gt;&#xA;&#xA;&#xA;&#xA;&lt;input type=&#34;radio&#34; class=&#34;toggle&#34; name=&#34;tabs-60209331&#34; id=&#34;tabs-60209331-0&#34; checked=&#34;checked&#34; /&gt;&#xA;&lt;label for=&#34;tabs-60209331-0&#34;&gt;Standard view&lt;/label&gt;&#xA;&lt;div class=&#34;book-tabs-content markdown-inner&#34;&gt;&#xA;&#xA;&lt;style type=&#34;text/css&#34;&gt;&#xA;#T_standard_d675a8ab6f2a8a27 th.col_heading {&#xA;  text-align: left;&#xA;  font-size: 1em;&#xA;}&#xA;#T_standard_d675a8ab6f2a8a27 td {&#xA;  text-align: left;&#xA;  font-size: 1em;&#xA;  padding: 1.5em;&#xA;}&#xA;&lt;/style&gt;&#xA;&lt;table id=&#34;T_standard_d675a8ab6f2a8a27&#34;&gt;&#xA;  &lt;thead&gt;&#xA;  &lt;/thead&gt;&#xA;  &lt;tbody&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row0_col0&#34; class=&#34;data row0 col0&#34; &gt;$ i = \sqrt{-1}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row1_col0&#34; class=&#34;data row1 col0&#34; &gt;$ z = a + ib$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row2_col0&#34; class=&#34;data row2 col0&#34; &gt;$\text{ mod z} =|z| = \sqrt{a^2+b^2} $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row3_col0&#34; class=&#34;data row3 col0&#34; &gt;$\tan \theta = \dfrac{b}{a} \text{ where } \theta \text{ equals arg z in interval } (-\pi, \pi]  \text{Don’t confuse with angle of a position vector which ranges from 0 to 360 degrees}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row4_col0&#34; class=&#34;data row4 col0&#34; &gt;$z = r (\cos \theta + i \sin \theta)$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row5_col0&#34; class=&#34;data row5 col0&#34; &gt;$ z_1z_2  = r_1 r_2[\cos(\theta_1 + \theta_2) + i \sin(\theta_1 + \theta_2)]$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row6_col0&#34; class=&#34;data row6 col0&#34; &gt;$ \dfrac{z_1}{z_2}  = \dfrac{r_1}{ r_2}[\cos(\theta_1 - \theta_2) + i \sin(\theta_1 - \theta_2)], z_2 \ne 0$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row7_col0&#34; class=&#34;data row7 col0&#34; &gt; $ z^n = r^n(\cos n \theta + i \sin n \theta)$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row8_col0&#34; class=&#34;data row8 col0&#34; &gt;$ z^{-1} = r^{-1}(\cos \theta - i \sin \theta), z \ne 0$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row9_col0&#34; class=&#34;data row9 col0&#34; &gt; $ z^{-n} = r^{-n}(\cos -n \theta + i \sin -n \theta), z \ne 0, n \in \mathbb{Z}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row10_col0&#34; class=&#34;data row10 col0&#34; &gt;$ z_1z_2...z_n = r_1r_2...r_n[\cos(\theta_1+\theta_2+...\theta_n) + i\sin(\theta_1+\theta_2+...\theta_n)]$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row11_col0&#34; class=&#34;data row11 col0&#34; &gt;$ |z_1||z_2| = |z_1z_2| \text{ and arg } z_1z_2 = { arg } z_1 + { arg } z_2$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row12_col0&#34; class=&#34;data row12 col0&#34; &gt;$ \dfrac{|z_1|}{|z_2|} = \bigg| \dfrac{z_1}{z_2} \bigg| \text{ and arg } \dfrac{z_1}{z_2} = { arg } z_1 - { arg } z_2$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row13_col0&#34; class=&#34;data row13 col0&#34; &gt;$ |z|^n = |z^n| \text{ and arg } z^n = n \times \text{arg z for } n \in \mathbb{Z}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row14_col0&#34; class=&#34;data row14 col0&#34; &gt;$ |z^{-1}| = \dfrac{1}{|z|} \text { and arg } z^{-1} = -\text {arg z}, z\ne 0$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row15_col0&#34; class=&#34;data row15 col0&#34; &gt;$ |z^{-n}| = \dfrac{1}{|z|^n} \text { and arg } z^{-n} = \text {-n arg z}, z\ne 0 \text { for } n \in \mathbb{Z}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row16_col0&#34; class=&#34;data row16 col0&#34; &gt;$ |z_1||z_2||z_3|...|z_n| = |z_1z_2z_3...z_n| \text { and arg } z_1z_2z_3...z_n = { arg } z_1 + { arg } z_2 + ...+ { arg } z_n \text { where } n \in \mathbb{Z}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row17_col0&#34; class=&#34;data row17 col0&#34; &gt;$ \text{ If } z = \cos \theta + i\sin \theta \text{, then } z^{-1} = \overline{z} \text{ (only applies when modulus = 1)} $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row18_col0&#34; class=&#34;data row18 col0&#34; &gt;$ z\overline{z} = |z|^2 \text{ and arg } z\overline{z} = 0  $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row19_col0&#34; class=&#34;data row19 col0&#34; &gt;$ |\overline{z}| = |z| \text{ and arg } \overline{z} = -{arg } z $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row20_col0&#34; class=&#34;data row20 col0&#34; &gt;$ \overline{z_1} + \overline{z_2} = \overline{z_1 + z_2}  $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row21_col0&#34; class=&#34;data row21 col0&#34; &gt;$ \overline{z_1} \,\, \overline{z_2} = \overline{z_1 z_2}  $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row22_col0&#34; class=&#34;data row22 col0&#34; &gt;$ z + \overline{z} = 2Re(z)  $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row23_col0&#34; class=&#34;data row23 col0&#34; &gt;$ z - \overline{z} = 2Im(z)  $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row24_col0&#34; class=&#34;data row24 col0&#34; &gt;$ {|z+w| \le |z| + |w|} \text{ (the triangle inequality where z and w are complex numbers)}  $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row25_col0&#34; class=&#34;data row25 col0&#34; &gt;$ e^{i\theta} = \cos \theta + i \sin \theta, \text{ where  }\theta \in \mathbb{R}  \text{ (This is Euler&#39;s formula and represents the exponential form of a complex number)}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row26_col0&#34; class=&#34;data row26 col0&#34; &gt;$ e^{i\pi} = -1  $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row27_col0&#34; class=&#34;data row27 col0&#34; &gt;$ z = a+ib = r(\cos \theta + i \sin \theta) = re^{i \theta}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row28_col0&#34; class=&#34;data row28 col0&#34; &gt;$ [r(\cos \theta + i \sin \theta)]^n = r^n(\cos n\theta + i\sin n\theta) =  r^ne^{in \theta}$&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row29_col0&#34; class=&#34;data row29 col0&#34; &gt;$(e ^ {i \theta})^n = e ^ {in \theta} $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row30_col0&#34; class=&#34;data row30 col0&#34; &gt;$z^n + \dfrac{1}{z^n} = 2 \cos n \theta $&lt;/td&gt;&#xA;    &lt;/tr&gt;&#xA;    &lt;tr&gt;&#xA;      &lt;td id=&#34;T_standard_d675a8ab6f2a8a27_row31_col0